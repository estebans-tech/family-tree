{
	"useTabs": true,
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"semi": false,
	"singleQuote": true,
	"trailingComma": "es5",
	"tailwindStylesheet": "./src/app.css"
}
